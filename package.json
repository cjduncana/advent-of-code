{
  "name": "advent-of-code",
  "version": "1.0.0",
  "description": "All my solutions for Advent of Code",
  "main": "index.js",
  "scripts": {
    "test": "cd 2017/ && elm test && elm analyse && elm format . --validate && cd ../",
    "test:watch": "node_modules/nodemon/bin/nodemon.js --exec npm test"
  },
  "nodemonConfig": {
    "ext": "elm",
    "ignore": [
      "elm-stuff"
    ],
    "watch": [
      "*.elm"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cjduncana/advent-of-code.git"
  },
  "keywords": [
    "elm",
    "javascript",
    "advent-of-code"
  ],
  "author": "Christopher Duncan <cj.duncan.a@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cjduncana/advent-of-code/issues"
  },
  "homepage": "https://github.com/cjduncana/advent-of-code#readme",
  "devDependencies": {
    "nodemon": "^1.12.1"
  }
}
